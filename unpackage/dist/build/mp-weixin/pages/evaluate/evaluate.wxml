<view><view class="header"><view data-event-opts="{{[['tap',[['returnBtn',['$event']]]]]}}" class="iconfont icon-fanhui left" bindtap="__e"></view><view class="title">商品评价</view><view data-event-opts="{{[['tap',[['shoppingBtn',['$event']]]]]}}" class="iconfont icon-gouwuche right" bindtap="__e"></view></view><view class="tags-container"><view class="rate"><view>{{"用户评价（"+tagsData.total+"）"}}</view><view><text>{{tagsData.rate}}</text>满意</view></view><view class="tags"><block wx:for="{{tagsData.tags}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view class="tag">{{''+(item.tag+' '+item.total)+''}}</view></block></view></view><view class="container"><block wx:for="{{contentData}}" wx:for-item="item" wx:for-index="__i1__" wx:key="id"><view class="item"><view class="head"><image src="{{item.head}}"></image></view><view class="content"><view class="name"><view>{{item.name}}</view><view class="iconfont icon-xiaolian2"><text>超爱</text></view></view><view class="text">{{item.content}}</view><view class="imgs"><block wx:for="{{item.imgs}}" wx:for-item="url" wx:for-index="__i2__" wx:key="*this"><view class="img"><image mode="aspectFit" src="{{url}}"></image></view></block></view><view class="time"><view>{{item.time}}</view><view class="icon"><view class="zan"><text>{{item.zan}}</text><text class="iconfont icon-zan active"></text></view><view class="pinglun"><text>{{item.pinglun}}</text><text class="iconfont icon-pinglun-copy"></text></view></view></view></view></view></block></view></view>