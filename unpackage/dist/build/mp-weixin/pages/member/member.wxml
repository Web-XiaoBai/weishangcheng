<view><view class="userInfo"><image class="bg" src="/static/user-bg.jpg"></image><view class="img"><image mode="aspectFit" src="{{userInfo.head||'/static/missing-face.png'}}"></image></view><block wx:if="{{isLogin}}"><view class="info"><view class="name">{{userInfo.name}}</view><view class="id">{{"用户ID："+userInfo.id}}</view></view></block><block wx:if="{{!isLogin}}"><view data-event-opts="{{[['tap',[['loginBtn',['$event']]]]]}}" class="login" bindtap="__e">登录/注册</view></block><view class="member"><view class="text"><text class="iconfont icon-huiyuan"></text>DCloud会员</view><view class="grant">立即开通</view></view></view><view class="balance_discount"><view class="balance box"><view>128.8</view><view class="gray">余额</view></view><view class="discount box"><view>0</view><view class="gray">优惠券</view></view><view class="integral box"><view>20</view><view class="gray">积分</view></view></view><view class="order_after"><block wx:for="{{orderNav}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view class="box"><view class="{{['iconfont',item.icon]}}"></view><view>{{item.name}}</view></view></block></view><view class="options_list"><block wx:for="{{options}}" wx:for-item="item" wx:for-index="__i1__" wx:key="name"><view data-event-opts="{{[['tap',[['navBtn',['$0','$1'],[[['options','name',item.name,'url']],[['options','name',item.name,'name']]]]]]]}}" class="item" bindtap="__e"><view class="{{['iconfont',item.icon]}}"></view><view class="name"><text class="tit">{{item.name}}</text><text class="iconfont icon-you"></text></view></view></block></view></view>