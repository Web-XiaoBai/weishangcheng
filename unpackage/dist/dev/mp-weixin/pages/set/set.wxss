page {
  background: #F8F8F8;
}
.header {
  background: #FFFFFF;
  padding: 0 30rpx;
  padding-top: calc(25px + 15rpx);
  padding-bottom: 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  position: relative;
  text-align: center;
  font-size: 30rpx;
  border-bottom: 1px solid #E5E5E5;
}
.header .iconfont {
  font-size: 38rpx;
}
.header .text {
  margin: 0 auto;
}
.set-list .set-title {
  padding: 26rpx 30rpx;
  font-size: 28rpx;
}
.set-list .set-item {
  padding: 0 30rpx;
  background: #FFFFFF;
}
.set-list .set-item .item {
  position: relative;
  padding: 26rpx 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  font-size: 24rpx;
  color: #333333;
  border-bottom: 1px solid #E5E5E5;
}
.set-list .set-item .item text {
  position: absolute;
  right: 50rpx;
  font-size: 24rpx;
  color: #8A8A8A;
}
.set-list .set-item .item:nth-last-child(1) {
  border: none;
}
.logout {
  margin: 30rpx auto;
  width: 650rpx;
  background: #FFFFFF;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
  color: #333333;
  text-align: center;
  font-size: 28rpx;
}

